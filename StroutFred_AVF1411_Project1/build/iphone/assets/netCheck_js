var saveData=require("db"),netCheck=function(e){var i=function(){var e=JSON.parse(this.responseText),i=e.current_observation,t=e.forecast.simpleforecast.forecastday[0],a=i.icon_url,o=i.display_location.full,n=i.feelslike_f,r=i.observation_time,d=i.weather,l=t.low.fahrenheit,c=t.high.fahrenheit,s=i.temp_f,T=i.temp_c,E=i.relative_humidity,p=i.precip_today_in,f=i.heat_index_f,u=i.pressure_in,m=i.wind_mph,h=i.wind_dir,D=i.wind_degrees;saveData.create(a,o,n,r,d,l,c,s,T,E,p,f,u,m,h,D)},t=function(e){alert("Error: "+e.error)};if(Ti.Network.online){var a=Ti.Database.open("localData");a.execute("DELETE FROM tblData"),a.close();var o=Ti.Network.createHTTPClient({onload:i,onerror:t});o.open("GET",e),o.send()}else alert("Network unavailable. Check your settings.")};exports.checkNet=netCheck;