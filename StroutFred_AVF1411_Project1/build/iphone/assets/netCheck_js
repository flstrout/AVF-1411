var saveData=require("db"),netCheck=function(e){var i=function(){var e=JSON.parse(this.responseText),i=e.current_observation,a=e.forecast.simpleforecast.forecastday[0];console.log(e);var t=i.icon,o=i.display_location.full,n=i.feelslike_f,r=i.observation_time,s=i.weather,l=a.low.fahrenheit,c=a.high.fahrenheit,d=i.temp_f,T=i.temp_c,f=i.relative_humidity,u=i.precip_today_in,E=i.heat_index_f,p=i.pressure_in,m=i.wind_mph,h=i.wind_dir,v=i.wind_degrees;if(i.observation_time_rfc822.substring(17,19)>=18||i.observation_time_rfc822.substring(17,19)<=5)var D=!0;else var D=!1;if(console.log("Night? = "+D),"clear"===t||"sunny"===t){if(D===!1)var w="/icons/Sun.gif";var w="/icons/Moon.gif"}else if("chancerain"===t||"rain"===t)var w="/icons/Rain.gif";else if("tstorms"===t||"unknown"===t)var w="/icons/Lightning.gif";else if("cloudy"===t)var w="/icons/Overcast.gif";else if("mostlycloudy"===t){if(D===!1)var w="/icons/PartlyCloudy.gif";var w="/icons/MoonCloudy.gif"}saveData.create(w,o,n,r,s,l,c,d,T,f,u,E,p,m,h,v)},a=function(e){alert("Error: "+e.error)};if(Ti.Network.online){var t=Ti.Database.open("localData");t.execute("DELETE FROM tblData"),t.close();var o=Ti.Network.createHTTPClient({onload:i,onerror:a});o.open("GET",e),o.send()}else alert("Network unavailable. Check your settings.")};exports.checkNet=netCheck;