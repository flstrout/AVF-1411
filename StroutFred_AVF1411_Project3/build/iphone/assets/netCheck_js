var database=require("database"),shData=function(e){var t="http://api.tvmaze.com/shows/"+e,a=function(){var e=JSON.parse(this.responseText),t=function(e){var t=e.id,a=e.summary,o=e.image.medium;database.update2(a,o,t)};t(e)},o=function(e){alert("Error: "+e.error)},i=Ti.Network.createHTTPClient({onload:a,onerror:o});i.open("GET",t),i.send()},netCheck=function(e){var t=function(e){var t=JSON.parse(this.responseText),a=function(t){for(e in t){var a=t[e].airdate,o=t[e].airtime.substring(0,2),i=t[e].airtime.substring(2,5),r=t[e].runtime,n=t[e].show.name,d=t[e].name,l=t[e].show.network.name,s=t[e].id,c=t[e].show.id;database.create(a,o,i,r,n,d,l,s,c),console.log(n+": "+d),console.log("From the API: "+s)}};a(t)},a=function(e){alert("Error: "+e.error)};if(Ti.Network.online){var o=Ti.Database.open("local");o.execute("DELETE FROM data"),o.close();var i=Ti.Network.createHTTPClient({onload:t,onerror:a});i.open("GET",e),i.send()}else alert("Network unavailable. Check your settings.")};exports.checkNet=netCheck;