var database=require("database"),netCheck=function(e){var t=function(e){var t=JSON.parse(this.responseText),a=function(t){for(e in t){var a=t[e].airdate,i=t[e].airtime.substring(0,2),r=t[e].airtime.substring(2,5),o=t[e].runtime,n=t[e].show.name,d=t[e].name,l=t[e].show.network.name;database.create(a,i,r,o,n,d,l)}};a(t)},a=function(e){alert("Error: "+e.error)};if(Ti.Network.online){var i=Ti.Database.open("local");i.execute("DELETE FROM data"),i.close();var r=Ti.Network.createHTTPClient({onload:t,onerror:a});r.open("GET",e),r.send()}else alert("Network unavailable. Check your settings.")};exports.checkNet=netCheck;