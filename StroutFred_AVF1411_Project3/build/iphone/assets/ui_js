var getDetail=require("detail"),fSize=14,defOp=.8,buildUI=function(e){var t=Ti.UI.createScrollView({scrollType:"vertical"}),a=Ti.UI.createLabel({text:"Primetime Channel Lineup",font:{fontSize:22,fontWeight:"bold"},top:2}),i=Ti.UI.createLabel({text:date.substring(5,7)+"/"+date.substring(8,12)+"/"+date.substring(0,4),font:{fontSize:32,fontWeight:"bold"}}),r=Ti.UI.createView({backgroundColor:"#fff",height:70,width:300,borderRadius:20,top:15,opacity:.8,layout:"vertical"}),d=Ti.UI.createTableView({top:45,backgroundColor:"#000",separatorColor:"#09f",width:"85%",borderRadius:6}),n=[],l=Ti.UI.createTableViewSection();n.push(l);for(o in e){var s=Ti.UI.createTableViewRow({showTitle:e[o].show,episode:e[o].episode,id1:e[o].id1,hasChild:!0,backgroundColor:"#eee",height:60});if(e[o].hh>12)var c="pm";else var c="am";if("00"===e[o].hh)var u=12;else if(e[o].hh>12)var u=e[o].hh-12;else var u=e[o].hh;var T=Ti.UI.createLabel({text:e[o].show+": "+e[o].episode,showTitle:e[o].show,episode:e[o].episode,id1:e[o].id1,font:{fontSize:22},top:25,left:20,width:"75%"}),p=Ti.UI.createLabel({text:e[o].network,showTitle:e[o].show,episode:e[o].episode,id1:e[o].id1,top:5,left:20}),h=Ti.UI.createLabel({text:u+e[o].mm+" "+c,showTitle:e[o].show,episode:e[o].episode,id1:e[o].id1,top:5,right:20}),f=Ti.UI.createLabel({text:Math.floor(e[o].tLength)+" min",showTitle:e[o].show,episode:e[o].episode,id1:e[o].id1,top:25,right:20});s.add(T),s.add(p),s.add(h),s.add(f),l.add(s),exports.sectionDetail=s}d.setData(n),d.addEventListener("click",function(e){getDetail.loadDetail(e.source)}),r.add(a),r.add(i);var m=Ti.UI.createWindow({layout:"vertical",top:5});m.add(r),m.add(t),t.add(d),m.open()};exports.buildUI=buildUI;