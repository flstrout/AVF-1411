var fSize=14,defOp=.8,buildUI=function(e){var t;for(t=0;t<e.length;t++){if(e[t].hh>12)var a="pm";else var a="am";if("00"===e[t].hh)var o=12;else if(e[t].hh>12)var o=e[t].hh-12;else var o=e[t].hh;var i=Ti.UI.createLabel({text:e[t].network,top:3,left:4,font:{fontSize:fSize}}),r=Ti.UI.createLabel({text:e[t].show+": "+e[t].episode,top:25,left:4,width:"78%",font:{fontSize:16,fontWeight:"bold"}}),d=Ti.UI.createLabel({text:Math.floor(e[t].tLength)+" min",top:27,right:4,font:{fontSize:fSize}}),n=Ti.UI.createLabel({text:o+e[t].mm+" "+a,top:3,right:4,font:{fontSize:fSize}}),l=Ti.UI.createView({top:5,width:"90%",height:50,borderColor:"#333",borderRadius:3,borderWidth:2,backgroundColor:"#fff",opacity:defOp});l.add(i),l.add(r),l.add(d),l.add(n)}masterView.add(l)};exports.buildUI=buildUI;