var ui=require("ui"),tblSavedData=[],read=function(){var e=Ti.Database.open("local");e.execute("CREATE TABLE IF NOT EXISTS data (id INTEGER PRIMARY KEY , date TEXT, hh TEXT, mm TEXT, length TEXT, show TEXT, episode TEXT, network TEXT)");for(var t=e.execute("SELECT id, date, hh, mm, length, show, episode, network FROM data");t.isValidRow();)tblSavedData.push({id:t.fieldByName("id"),date:t.fieldByName("date"),hh:t.fieldByName("hh"),mm:t.fieldByName("mm"),tLength:t.fieldByName("length"),show:t.fieldByName("show"),episode:t.fieldByName("episode"),network:t.fieldByName("network")}),t.next();t.close(),e.close(),ui.buildUI(tblSavedData)};exports.read=read;var create=function(e,t,a,i,o,r,d){var l=Ti.Database.open("local");l.execute("CREATE TABLE IF NOT EXISTS data (id INTEGER PRIMARY KEY , date TEXT, hh TEXT, mm TEXT, length TEXT, show TEXT, episode TEXT, network TEXT)"),l.execute("INSERT INTO data (date, hh, mm, length, show, episode, network) VALUES (?, ?, ?, ?, ?, ?, ?)",e,t,a,i,o,r,d);l.lastInsertRowId;l.close(),tblSavedData=[],read()};exports.create=create,exports.tblSavedData=tblSavedData;