var tzMod=[],read=function(){var e=Ti.Database.open("local");e.execute("CREATE TABLE IF NOT EXISTS mod (id INTEGER PRIMARY KEY , modifier TEXT)");for(var t=e.execute("SELECT id, modifier FROM mod");t.isValidRow();)tzMod.push({id:t.fieldByName("id"),modifier:t.fieldByName("modifier")}),t.next();t.close(),e.close(),console.log(tzMod.modifier)};exports.read=read;var create=function(e){var t=Ti.Database.open("local");t.execute("CREATE TABLE IF NOT EXISTS mod (id INTEGER PRIMARY KEY , modifier TEXT)"),t.execute("INSERT INTO mod (modifier) VALUES (?)",e);t.lastInsertRowId;t.close(),tzMod=[],read()};exports.create=create,exports.tzMod=tzMod;