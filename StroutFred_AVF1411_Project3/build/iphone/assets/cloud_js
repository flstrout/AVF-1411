var myCloud=require("ti.cloud");myCloud.debug=!0;var loginUser=function(){myCloud.Users.login({login:"FredStrout617@fullsail.edu",password:"Dennis44!"},function(e){e.success?getLocation.currentLocation():alert(e.error&&e.message||JSON.stringify(e))})},updateCloud=function(e,t,i,a,r,o,l,n,d,s,c,u,g,m,y,h,w){myCloud.Objects.create({classname:"history",fields:{icon:e,location:t,feels:i,last:a,condition:r,min:o,max:l,fahrenheit:n,celsius:d,humidity:s,precip:c,heat:u,press:g,windSpeed:m,windDirection:y,windDegrees:h,night:w}},function(e){alert(e.success?"Titanium Cloud Updated":e.error&&e.message||JSON.stringify(e))})};exports.updateCloud=updateCloud;