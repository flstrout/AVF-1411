var myCloud=require("ti.cloud");myCloud.debug=!0;var loginUser=function(){myCloud.Users.login({login:"FredStrout617@fullsail.edu",password:"Dennis44!"},function(e){e.success?getLocation.currentLocation():alert(e.error&&e.message||JSON.stringify(e))})},updateCloud=function(e,t,a,i,o,r,n,l,s,d,c,u,g,m,h,p,f){myCloud.Objects.create({classname:"history",fields:{icon:e,location:t,feels:a,last:i,condition:o,min:r,max:n,fahrenheit:l,celsius:s,humidity:d,precip:c,heat:u,press:g,windSpeed:m,windDirection:h,windDegrees:p,night:f}},function(e){alert(e.success?"Titanium Cloud Updated":e.error&&e.message||JSON.stringify(e))})};exports.updateCloud=updateCloud;