var loadDetail=function(e){console.log("EpisodeID : "+Math.floor(e.id1));var t=function(t){var a="http://api.tvmaze.com/episodes/"+t,o=function(){var t=JSON.parse(this.responseText),a=function(t){{var a="Episode Synopsis: "+t.summary.replace(/<[^>]+>/g,""),o=(t.episode,t.airtime.substring(0,2));t.airtime.substring(2,5)}if(o>12);else;if("00"===o);else if(o>12);else;var i=function(){d.close()},d=Ti.UI.createWindow({backgroundColor:"#09f",title:"Epsiode Details",height:"35%",width:"90%",top:"25%",borderRadius:6}),r=Ti.UI.createLabel({text:"Close",font:{fontSize:18,fontWeight:"bold"},color:"#222",textAlign:"center",backgroundColor:"#6fc",borderColor:"#222",borderRadius:4,top:300,height:35,width:120});r.addEventListener("click",i);var l=Ti.UI.createLabel({text:e.episode,font:{fontSize:26,fontWeight:"bold"},color:"#666",top:60,left:28}),n=Ti.UI.createLabel({text:e.showTitle,font:{fontSize:30,fontWeight:"bold"},color:"#222",top:25,left:20}),s=Ti.UI.createLabel({text:a,font:{fontSize:18,fontFamily:"Helvetica Neue",fontWeight:"bold"},color:"#ffcc00",textAlign:"left",verticalAlign:1,top:95,width:"80%",height:200});d.add(r),d.add(s),d.add(n),d.add(l),d.open()};a(t)},i=function(e){alert("Error: "+e.error)},d=Ti.Network.createHTTPClient({onload:o,onerror:i});d.open("GET",a),d.send()};t(JSON.stringify(Math.floor(e.id1)))};exports.loadDetail=loadDetail;