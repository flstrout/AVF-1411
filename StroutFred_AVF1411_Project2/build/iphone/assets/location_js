var getNetCheck=require("netCheck"),currentLocation=function(){Ti.Geolocation.ACCURACY_HIGH,Ti.Geolocation.getCurrentPosition(function(e){if("android"!=Ti.Platform.osname)var i=e.coords.latitude,t=e.coords.longitude;else var i=37.3318,t=-122.029583;var n=i+","+t;console.log(n);var a="http://api.wunderground.com/api/4af8df4427496b2f/conditions/forecast/q/"+n+".json";getNetCheck.checkNet(a)})};exports.currentLocation=currentLocation;