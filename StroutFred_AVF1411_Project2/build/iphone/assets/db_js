var openUI=require("ui"),tblConditions=[],read=function(){var e=Ti.Database.open("localData");e.execute("CREATE TABLE IF NOT EXISTS tblData (id INTEGER PRIMARY KEY , icon TEXT, location TEXT, feels TEXT, last TEXT, condition TEXT, min TEXT, max TEXT, fahrenheit TEXT, celsius TEXT, humidity TEXT, precip TEXT, heat TEXT, press TEXT, windSpeed TEXT, windDirection TEXT, windDegrees TEXT, night TEXT)");for(var i=e.execute("SELECT id, icon, location, feels, last, condition, min, max, fahrenheit, celsius, humidity, precip, heat, press, windSpeed, windDirection, windDegrees, night FROM tblData");i.isValidRow();)tblConditions.push({id:i.fieldByName("id"),icon:i.fieldByName("icon"),location:i.fieldByName("location"),feels:i.fieldByName("feels"),last:i.fieldByName("last"),condition:i.fieldByName("condition"),min:i.fieldByName("min"),max:i.fieldByName("max"),fahrenheit:i.fieldByName("fahrenheit"),celsius:i.fieldByName("celsius"),humidity:i.fieldByName("humidity"),precip:i.fieldByName("precip"),heat:i.fieldByName("heat"),press:i.fieldByName("press"),windSpeed:i.fieldByName("windSpeed"),windDirection:i.fieldByName("windDirection"),windDegrees:i.fieldByName("windDegrees"),night:i.fieldByName("night")}),i.next();i.close(),e.close();var n=tblConditions[0];openUI.buildUI(n.icon,n.location,n.feels,n.last,n.condition,n.min,n.max,n.fahrenheit,n.celsius,n.humidity,n.precip,n.heat,n.press,n.windSpeed,n.windDirection,n.windDegrees)};exports.read=read;var create=function(e,i,n,t,a,o,d,l,s,r,T,c,u,E,m,p,f){var h=Ti.Database.open("localData");h.execute("CREATE TABLE IF NOT EXISTS tblData (id INTEGER PRIMARY KEY , icon TEXT, location TEXT, feels TEXT, last TEXT, condition TEXT, min TEXT, max TEXT, fahrenheit TEXT, celsius TEXT, humidity TEXT, precip TEXT, heat TEXT, press TEXT, windSpeed TEXT, windDirection TEXT, windDegrees TEXT, night TEXT)"),h.execute("INSERT INTO tblData (icon, location, feels, last, condition, min, max, fahrenheit, celsius, humidity, precip, heat, press, windSpeed, windDirection, windDegrees, night) VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?)",e,i,n,t,a,o,d,l,s,r,T,c,u,E,m,p,f);h.lastInsertRowId;h.close(),tblConditions=[],read()};exports.create=create;var del=function(e){var i=Ti.Database.open("localData");i.execute("DELETE FROM tblData WHERE id=?",e),i.close(),tblConditions=[],read()};exports.del=del,exports.tblConditions=tblConditions;