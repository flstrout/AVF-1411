var saveData=require("db"),netCheck=function(e){var i=function(){var e=JSON.parse(this.responseText),i=e.current_observation,n=e.forecast.simpleforecast.forecastday[0];console.log(e);var a=i.icon,t=i.display_location.full,o=i.feelslike_f,r=i.observation_time,s=i.weather,l=n.low.fahrenheit,d=n.high.fahrenheit,c=i.temp_f,T=i.temp_c,u=i.relative_humidity,f=i.precip_today_in,p=i.heat_index_f,h=i.pressure_in,m=i.wind_mph,E=i.wind_dir,g=i.wind_degrees;if(i.observation_time_rfc822.substring(17,19)>=18||i.observation_time_rfc822.substring(17,19)<=5)var y=!0;else var y=!1;if("clear"===a||"sunny"===a)if(y===!1)var w="/icons/Sun/Sun";else var w="/icons/Moon/Moon";else if("chancerain"===a||"rain"===a)var w="/icons/Rain/Rain";else if("tstorms"===a||"unknown"===a)var w="/icons/Lightning/Lightning";else if("cloudy"===a)var w="/icons/Overcast/Overcast";else if(y===!1)var w="/icons/PartlyCloudy/PartlyCloudy";else var w="/icons/MoonCloudy/MoonCloudy";saveData.create(w,t,o,r,s,l,d,c,T,u,f,p,h,m,E,g,y),loginCloud.updateCloud(w,t,o,r,s,l,d,c,T,u,f,p,h,m,E,g,y)},n=function(e){alert("Error: "+e.error)};if(Ti.Network.online){var a=Ti.Database.open("localData");a.execute("DELETE FROM tblData"),a.close();var t=Ti.Network.createHTTPClient({onload:i,onerror:n});t.open("GET",e),t.send()}else alert("Network unavailable. Check your settings.")};exports.checkNet=netCheck;