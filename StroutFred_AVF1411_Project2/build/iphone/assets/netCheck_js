var saveData=require("db"),netCheck=function(e){var i=function(){var e=JSON.parse(this.responseText),i=e.current_observation,t=e.forecast.simpleforecast.forecastday[0],n=i.icon,a=i.display_location.full,o=i.feelslike_f,r=i.observation_time,s=i.weather,l=t.low.fahrenheit,d=t.high.fahrenheit,c=i.temp_f,T=i.temp_c,u=i.relative_humidity,f=i.precip_today_in,p=i.heat_index_f,h=i.pressure_in,m=i.wind_mph,E=i.wind_degrees,g=i.wind_degrees;if(i.observation_time_rfc822.substring(17,19)>=18||i.observation_time_rfc822.substring(17,19)<=5)var y=!0;else var y=!1;if("clear"===n||"sunny"===n)if(y===!1)var w="/icons/Sun/Sun";else var w="/icons/Moon/Moon";else if("chancerain"===n||"rain"===n)var w="/icons/Rain/Rain";else if("tstorms"===n||"unknown"===n)var w="/icons/Lightning/Lightning";else if("cloudy"===n)var w="/icons/Overcast/Overcast";else if(y===!1)var w="/icons/PartlyCloudy/PartlyCloudy";else var w="/icons/MoonCloudy/MoonCloudy";saveData.create(w,a,o,r,s,l,d,c,T,u,f,p,h,m,E,g,y),loginCloud.updateCloud(w,a,o,r,s,l,d,c,T,u,f,p,h,m,E,g,y)},t=function(e){alert("Error: "+e.error)};if(Ti.Network.online){var n=Ti.Database.open("localData");n.execute("DELETE FROM tblData"),n.close();var a=Ti.Network.createHTTPClient({onload:i,onerror:t});a.open("GET",e),a.send()}else alert("Network unavailable. Check your settings.")};exports.checkNet=netCheck;