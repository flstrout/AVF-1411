var saveData=require("db"),netCheck=function(e){var i=function(){var e=JSON.parse(this.responseText),i=e.current_observation,n=e.forecast.simpleforecast.forecastday[0];console.log(e);var a=i.icon,t=i.display_location.full,o=i.feelslike_f,r=i.observation_time,s=i.weather,l=n.low.fahrenheit,d=n.high.fahrenheit,c=i.temp_f,T=i.temp_c,u=i.relative_humidity,f=i.precip_today_in,p=i.heat_index_f,m=i.pressure_in,h=i.wind_mph,E=i.wind_dir,g=i.wind_degrees;if(i.observation_time_rfc822.substring(17,19)>=18||i.observation_time_rfc822.substring(17,19)<=5)var w=!0;else var w=!1;if("clear"===a||"sunny"===a)if(w===!1)var y="/icons/Sun.gif";else var y="/icons/Moon.gif";else if("chancerain"===a||"rain"===a)var y="/icons/Rain.gif";else if("tstorms"===a||"unknown"===a)var y="/icons/Lightning.gif";else if("cloudy"===a)var y="/icons/Overcast.gif";else if(w===!1)var y="/icons/PartlyCloudy.gif";else var y="/icons/MoonCloudy.gif";saveData.create(y,t,o,r,s,l,d,c,T,u,f,p,m,h,E,g,w),loginCloud.updateCloud(y,t,o,r,s,l,d,c,T,u,f,p,m,h,E,g,w)},n=function(e){alert("Error: "+e.error)};if(Ti.Network.online){var a=Ti.Database.open("localData");a.execute("DELETE FROM tblData"),a.close();var t=Ti.Network.createHTTPClient({onload:i,onerror:n});t.open("GET",e),t.send()}else alert("Network unavailable. Check your settings.")};exports.checkNet=netCheck;