var myCloud=require("ti.cloud");myCloud.debug=!0;var loginUser=function(){myCloud.Users.login({login:"FredStrout617@fullsail.edu",password:"Dennis44!"},function(e){e.success?getLocation.currentLocation():alert(e.error&&e.message||JSON.stringify(e))})};Ti.Network.online===!0?loginUser():alert("Network unavailable. Check your settings.");var updateCloud=function(e,i,o,a,t,r,n,l,s,u,d,c,g,p,m,w,C){myCloud.Objects.create({classname:"history",fields:{icon:e,location:i,feels:o,last:a,condition:t,min:r,max:n,fahrenheit:l,celsius:s,humidity:u,precip:d,heat:c,press:g,windSpeed:p,windDirection:m,windDegrees:w,night:C}},function(e){alert(e.success?"Titanium Cloud Updated":e.error&&e.message||JSON.stringify(e))})};exports.updateCloud=updateCloud;