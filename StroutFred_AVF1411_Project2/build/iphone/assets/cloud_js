var myCloud=require("ti.cloud");myCloud.debug=!0;var loginUser=function(){var e=require("location");myCloud.Users.login({login:"FredStrout617@fullsail.edu",password:"Dennis44!"},function(i){i.success?e.currentLocation():alert(i.error&&i.message||JSON.stringify(i))})};Ti.Network.online===!0?loginUser():alert("Network unavailable. Check your settings.");var updateCloud=function(e,i,a,o,r,n,t,l,s,u,d,c,g,p,m,w,C){myCloud.Objects.create({classname:"history",fields:{icon:e,location:i,feels:a,last:o,condition:r,min:n,max:t,fahrenheit:l,celsius:s,humidity:u,precip:d,heat:c,press:g,windSpeed:p,windDirection:m,windDegrees:w,night:C}},function(e){alert(e.success?"Titanium Cloud Updated":e.error&&e.message||JSON.stringify(e))})};exports.updateCloud=updateCloud;